{
    "name": "diego-drese/laravel-datatables-mongodb",
    "description": "Laravel 11 DataTables plugin to support Mongodb",
    "keywords": [
        "laravel",
        "dataTables",
        "mongodb"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "pimlie",
            "email": "pimlie@hotmail.com"
        },{
            "name": "diegodrese",
            "email": "diego.neumann.drese@gmail.com"
        }

    ],
    "require": {
        "yajra/laravel-datatables": "^11.0",
        "mongodb/laravel-mongodb": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "DiegoDrese\\DataTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DiegoDrese\\DataTables\\Tests\\": "tests/"
        }
    },
  "extra": {
        "laravel": {
          "providers": [
            "DiegoDrese\\DataTables\\MongodbDataTablesServiceProvider"
          ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "Orchestra/Testbench": "^3.5",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "vendor/bin/phpcs src/*"
    }
}
